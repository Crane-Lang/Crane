pub fn main() {
    greet("world")
    greet("trees")
    greet("everyone")
    println("1 + 1 =")
    println(int_to_string(int_add(1, 1)))
}

fn greet(name) {
    join("Hello", name)
    print("!")
    println("")
}

fn join(a, b) {
    print(a)
    print(", ")
    print(b)
}
